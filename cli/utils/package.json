{
  "name": "@repodog/cli-utils",
  "type": "module",
  "version": "0.1.0",
  "description": "The RepoDog cli utils module.",
  "homepage": "https://github.com/badbatch/repodog",
  "bugs": {
    "url": "https://github.com/badbatch/repodog/issues"
  },
  "repository": {
    "directory": "cli/utils",
    "type": "git",
    "url": "https://github.com/badbatch/repodog/cli/utils"
  },
  "license": "MIT",
  "author": "Dylan Aubrey",
  "main": "./dist/main/index.mjs",
  "types": "./dist/types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm run clean:dist && pnpm run compile",
    "clean:deps": "del-cli ./node_modules",
    "clean:dist": "del-cli ./dist",
    "compile": "pnpm run /^compile:.*/",
    "compile:main": "rollup -c ../../rollup.config.cjs",
    "compile:types": "tsc --project ./tsconfig.json"
  },
  "dependencies": {
    "ansi-colors": "4.1.3",
    "glob": "8.1.0",
    "js-yaml": "4.1.0",
    "semver": "7.1.3",
    "shelljs": "0.8.1"
  },
  "peerDependencies": {
    "@babel/runtime": "< 8",
    "core-js": "< 4"
  },
  "devDependencies": {
    "@jest/globals": "29.3.1",
    "@repodog/cli-test-utils": "workspace:../test-utils",
    "@types/glob": "8.0.1",
    "@types/js-yaml": "4.0.5",
    "@types/semver": "7.1.0",
    "@types/shelljs": "0.8.11",
    "del-cli": "3.0.0",
    "type-fest": "3.5.1"
  }
}
