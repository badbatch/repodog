{
  "name": "@repodog/cli-new",
  "type": "module",
  "version": "1.0.4-alpha.0",
  "description": "The RepoDog cli new module.",
  "homepage": "https://github.com/badbatch/repodog",
  "bugs": {
    "url": "https://github.com/badbatch/repodog/issues"
  },
  "repository": {
    "directory": "cli/new",
    "type": "git",
    "url": "https://github.com/badbatch/repodog/cli/new"
  },
  "license": "MIT",
  "author": "Dylan Aubrey",
  "main": "./dist/main/index.mjs",
  "types": "./dist/types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm run clean:dist && pnpm run compile",
    "clean:deps": "del-cli ./node_modules",
    "clean:dist": "del-cli ./dist",
    "compile": "pnpm run /^compile:.*/",
    "compile:main": "rollup -c ./rollup.config.cjs",
    "compile:types": "tsc --project ./tsconfig.build.json",
    "hygen": "hygen"
  },
  "dependencies": {
    "@repodog/cli-utils": "workspace:../utils",
    "@repodog/cli-setup": "workspace:../setup",
    "ansi-colors": "^4.1.3",
    "enquirer": "^2.3.6",
    "hygen": "^6.2.11",
    "shelljs": "^0.8.4",
    "yargs": "^15.1.0"
  },
  "peerDependencies": {
    "@babel/runtime": "< 8",
    "core-js": "< 4",
    "lodash": "< 5"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@repodog/cli-test-utils": "workspace:../test-utils",
    "@repodog/rollup-config": "workspace:../../configs/rollup-config",
    "@types/lodash": "^4.14.191",
    "@types/shelljs": "^0.8.11",
    "@types/yargs": "^15.0.4",
    "del-cli": "^3.0.0",
    "lodash": "^4.17.21"
  }
}