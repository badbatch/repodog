{
  "name": "@repodog/cli-publish",
  "description": "The RepoDog cli publish module.",
  "version": "1.1.10",
  "author": "Dylan Aubrey",
  "license": "MIT",
  "homepage": "https://github.com/badbatch/repodog",
  "repository": {
    "directory": "cli/publish",
    "type": "git",
    "url": "https://github.com/badbatch/repodog/cli/publish"
  },
  "bugs": "https://github.com/badbatch/repodog/issues",
  "type": "module",
  "main": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm run clean:dist && pnpm run compile",
    "clean:deps": "del-cli ./node_modules",
    "clean:dist": "del-cli ./dist",
    "compile": "pnpm run /^compile:.*/",
    "compile:main": "rollup -c ../../rollup.config.cjs",
    "compile:types": "tsc --project ./tsconfig.build.json"
  },
  "dependencies": {
    "@repodog/cli-utils": "workspace:*",
    "ansi-colors": "^4.1.3",
    "semver": "^7.1.3",
    "shelljs": "^0.8.4",
    "yargs": "^15.1.0"
  },
  "peerDependencies": {
    "@babel/runtime": "<8",
    "core-js": "<4"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@repodog/cli-test-utils": "workspace:*",
    "@types/semver": "^7.1.0",
    "@types/shelljs": "^0.8.11",
    "@types/yargs": "^15.0.4",
    "del-cli": "^3.0.0"
  }
}
