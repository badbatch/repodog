{
  "name": "@repodog/cli-write",
  "type": "module",
  "version": "0.0.2-alpha.0",
  "description": "The RepoDog cli write module.",
  "homepage": "https://github.com/badbatch/repodog",
  "bugs": {
    "url": "https://github.com/badbatch/repodog/issues"
  },
  "repository": {
    "directory": "cli/write",
    "type": "git",
    "url": "https://github.com/badbatch/repodog/cli/write"
  },
  "license": "MIT",
  "author": "Dylan Aubrey",
  "main": "./dist/main/index.mjs",
  "types": "./dist/types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm run clean:dist && pnpm run compile",
    "clean:deps": "del-cli ./node_modules",
    "clean:dist": "del-cli ./dist",
    "compile": "pnpm run /^compile:.*/",
    "compile:main": "rollup -c ../../rollup.config.cjs",
    "compile:types": "tsc --project ./tsconfig.json"
  },
  "dependencies": {
    "@repodog/cli-utils": "workspace:../utils",
    "ansi-colors": "4.1.3",
    "dotenv": "^16.0.3",
    "openai": "3.2.1",
    "shelljs": "0.8.1",
    "yargs": "15.1.0"
  },
  "peerDependencies": {
    "@babel/runtime": "< 8",
    "core-js": "< 4"
  },
  "devDependencies": {
    "@jest/globals": "29.3.1",
    "@repodog/cli-test-utils": "workspace:../test-utils",
    "@types/shelljs": "0.8.11",
    "@types/yargs": "15.0.4",
    "del-cli": "3.0.0"
  }
}