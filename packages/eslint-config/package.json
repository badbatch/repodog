{
  "name": "@repodog/eslint-config",
  "version": "0.0.3",
  "description": "The RepoDog eslint-config module.",
  "author": "Dylan Aubrey <dylanaubrey@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/dylanaubrey/repodog",
  "bugs": {
    "url": "https://github.com/dylanaubrey/repodog/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dylanaubrey/repodog"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "keywords": [
    "eslint",
    "configuration"
  ],
  "scripts": {
    "compile": "concurrently npm:compile:*",
    "compile:main": "cross-env BABEL_ENV=main babel ./src --out-dir ./lib --extensions \".js\" --config-file ../../babel.config.js"
  },
  "dependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.14.0"
  }
}
