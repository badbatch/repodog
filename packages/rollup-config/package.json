{
  "name": "@repodog/rollup-config",
  "version": "0.0.3",
  "description": "The RepoDog rollup-config module.",
  "author": "Dylan Aubrey <dylanaubrey@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/dylanaubrey/repodog",
  "bugs": {
    "url": "https://github.com/dylanaubrey/repodog/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dylanaubrey/repodog"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "keywords": [
    "rollup",
    "configuration"
  ],
  "scripts": {
    "compile": "concurrently npm:compile:*",
    "compile:main": "cross-env BABEL_ENV=main babel ./src --out-dir ./lib --extensions \".js\" --config-file ../../babel.config.js"
  },
  "dependencies": {
    "rollup": "^1.17.0",
    "rollup-plugin-analyzer": "^3.1.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.1"
  }
}