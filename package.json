{
  "name": "repodog",
  "version": "1.7.47",
  "author": "Dylan Aubrey",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^22",
    "pnpm": "^9"
  },
  "scripts": {
    "build": "pnpm run -r build",
    "clean:deps": "pnpm run -r clean:deps && del-cli ./node_modules",
    "clean:dist": "pnpm run -r clean:dist",
    "commit": "commit",
    "cut:changelog": "changelog",
    "lint": "pnpm run /^lint:.*/",
    "lint:code": "eslint .",
    "lint:docs": "markdownlint-cli2 --config \".markdownlint.json\" \"**/*.md\" \"!**/node_modules/**\"",
    "postinstall": "sh shellScripts/installActivateMise.sh",
    "prepare": "husky",
    "repodog": "repodog",
    "syncpack": "syncpack format && syncpack list-mismatches && syncpack lint-semver-ranges",
    "test": "node --require=suppress-experimental-warnings --experimental-vm-modules node_modules/jest/bin/jest.js",
    "type-check": "tsc --noEmit",
    "validate": "pnpm run syncpack && pnpm run validate:babel && pnpm run validate:swc && pnpm run lint && pnpm run type-check",
    "validate:babel": "COMPILER=babel pnpm run build && COMPILER=babel pnpm run test",
    "validate:swc": "pnpm run build && pnpm run test"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/runtime": "^7.25.6",
    "@commitlint/cli": "^19.5.0",
    "@jest/globals": "^29.7.0",
    "@repodog/babel-config": "workspace:*",
    "@repodog/cli": "workspace:*",
    "@repodog/commitlint-config": "workspace:*",
    "@repodog/eslint-config": "workspace:*",
    "@repodog/eslint-config-jest": "workspace:*",
    "@repodog/jest-config": "workspace:*",
    "@repodog/markdownlint-config": "workspace:*",
    "@repodog/prettier-config": "workspace:*",
    "@repodog/rollup-config": "workspace:*",
    "@repodog/swc-config": "workspace:*",
    "@repodog/syncpack-config": "workspace:*",
    "@repodog/ts-config": "workspace:*",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-swc": "^0.3.0",
    "@swc/core": "^1.7.26",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.5.5",
    "core-js": "^3.38.1",
    "del-cli": "^5.1.0",
    "eslint": "^9.11.0",
    "generate-changelog": "^1.8.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "markdownlint-cli2": "^0.14.0",
    "prettier": "^3.3.3",
    "rollup": "^4.22.4",
    "suppress-experimental-warnings": "^2.0.0",
    "syncpack": "^13.0.0",
    "typescript": "^5.6.2"
  },
  "pnpm": {
    "overrides": {
      "body-parser": ">=1.20.3",
      "braces": ">=3.0.3",
      "cookie": ">=0.7.0",
      "ejs": ">=3.1.10",
      "express": ">=4.20.0",
      "follow-redirects": ">=1.15.6",
      "micromatch": ">=4.0.8",
      "path-to-regexp": ">=0.1.10",
      "send": ">=0.19.0",
      "serve-static": ">=1.16.0",
      "ws": ">=8.17.1"
    }
  }
}