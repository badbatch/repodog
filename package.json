{
  "name": "repodog",
  "description": "An command line module for managing repositories and a set of configs for popular tooling.",
  "version": "2.0.6",
  "author": "miami-man",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/badbatch/repodog",
  "repository": "badbatch/repodog",
  "bugs": "https://github.com/badbatch/repodog/issues",
  "type": "module",
  "engines": {
    "node": "^22",
    "pnpm": "^10"
  },
  "scripts": {
    "build": "pnpm run -r build",
    "clean:deps": "pnpm run -r clean:deps && del-cli ./node_modules",
    "clean:dist": "pnpm run -r clean:dist",
    "cut:changelog": "changelog",
    "installActivateMise": "sh shellScripts/installActivateMise.sh",
    "lint": "pnpm run /^lint:.*/",
    "lint:code": "eslint .",
    "lint:docs": "markdownlint-cli2 --config \".markdownlint.json\" \"**/*.md\" \"!**/node_modules/**\"",
    "prepare": "husky",
    "repodog": "repodog",
    "syncpack": "syncpack format && syncpack list-mismatches && syncpack lint-semver-ranges",
    "test": "node --require=suppress-experimental-warnings --experimental-vm-modules node_modules/jest/bin/jest.js",
    "type-check": "tsc --noEmit",
    "validate": "pnpm run syncpack && pnpm run validate:babel && pnpm run validate:swc && pnpm run lint && pnpm run type-check",
    "validate:babel": "COMPILER=babel pnpm run build && COMPILER=babel pnpm run test",
    "validate:swc": "pnpm run build && pnpm run test"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@commitlint/cli": "^19.8.1",
    "@jest/globals": "^29.7.0",
    "@repodog/babel-config": "workspace:*",
    "@repodog/cli": "workspace:*",
    "@repodog/commitlint-config": "workspace:*",
    "@repodog/eslint-config": "workspace:*",
    "@repodog/eslint-config-jest": "workspace:*",
    "@repodog/jest-config": "workspace:*",
    "@repodog/markdownlint-config": "workspace:*",
    "@repodog/prettier-config": "workspace:*",
    "@repodog/rollup-config": "workspace:*",
    "@repodog/swc-config": "workspace:*",
    "@repodog/syncpack-config": "workspace:*",
    "@repodog/ts-config": "workspace:*",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-swc": "^0.3.0",
    "@swc/core": "^1.13.3",
    "@swc/jest": "^0.2.39",
    "@types/jest": "^29.5.13",
    "@types/node": "^24.2.1",
    "core-js": "^3.45.0",
    "del-cli": "^6.0.0",
    "eslint": "^9.33.0",
    "generate-changelog": "^1.8.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "markdownlint-cli2": "^0.18.1",
    "prettier": "^3.6.2",
    "rollup": "^4.46.2",
    "suppress-experimental-warnings": "^2.0.0",
    "syncpack": "^13.0.4",
    "typescript": "^5.9.2"
  },
  "pnpm": {
    "overrides": {
      "@babel/helpers": ">=7.26.10",
      "@babel/runtime": ">=7.26.10",
      "axios": ">=1.8.2",
      "body-parser": ">=1.20.3",
      "brace-expansion": ">=1.1.12",
      "braces": ">=3.0.3",
      "cookie": ">=0.7.0",
      "cross-spawn": ">=7.0.5",
      "ejs": ">=3.1.10",
      "esbuild": ">=0.25.0",
      "express": ">=4.20.0",
      "follow-redirects": ">=1.15.6",
      "form-data": ">=4.0.4",
      "micromatch": ">=4.0.8",
      "nanoid": ">=3.3.8",
      "path-to-regexp": ">=0.1.12",
      "send": ">=0.19.0",
      "serialize-javascript": ">=6.0.2",
      "serve-static": ">=1.16.0",
      "tmp": ">=0.2.4",
      "ws": ">=8.17.1"
    }
  }
}