{
  "name": "repodog",
  "version": "0.0.3",
  "description": "An opinionated command line utility for managing a repository.",
  "author": "Dylan Aubrey <dylanaubrey@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/dylanaubrey/repodog",
  "bugs": {
    "url": "https://github.com/dylanaubrey/repodog/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dylanaubrey/repodog"
  },
  "keywords": [
    "command line",
    "cli",
    "lerna",
    "yarn",
    "releases",
    "monorepos",
    "repositories"
  ],
  "bin": {
    "new-package": "bin/new-package.js"
  },
  "scripts": {
    "build": "yarn run clean && yarn run test && yarn run compile",
    "changelog": "changelog",
    "clean": "del lib",
    "compile": "gulp compile",
    "cutoff": "cutoff",
    "dts-gen": "dts-gen -d \"types\" -m",
    "jest": "jest",
    "lint": "yarn run tslint && yarn run markdownLint",
    "markdownLint": "markdownlint README.md",
    "publish-cutoff": "publish-cutoff",
    "test": "yarn run lint && yarn run type-check && yarn run jest",
    "tslint": "gulp tslint",
    "type-check": "gulp type-check"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run test"
    }
  },
  "resolutions": {
    "babel-core": "^7.0.0-0"
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0-beta.44",
    "chalk": "^2.4.1",
    "cutoff": "^0.0.15",
    "fs-extra": "^6.0.1",
    "semver": "^5.5.0",
    "shelljs": "^0.8.1",
    "validate-npm-package-name": "^3.0.0",
    "yargs": "^12.0.1"
  },
  "peerDependencies": {
    "lerna": "^2.9.0",
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.44",
    "@babel/core": "7.0.0-beta.44",
    "@babel/preset-env": "7.0.0-beta.44",
    "@babel/preset-stage-0": "7.0.0-beta.44",
    "@types/dotenv": "^4.0.3",
    "@types/fs-extra": "^5.0.3",
    "@types/jest": "^23.1.3",
    "@types/lodash": "^4.14.110",
    "@types/node": "^10.5.1",
    "@types/semver": "^5.5.0",
    "@types/shelljs": "^0.8.0",
    "@types/yargs": "^11.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.2.0",
    "babel-plugin-lodash": "^3.3.4",
    "codecov": "^3.0.0",
    "cross-env": "^5.1.6",
    "del-cli": "^1.1.0",
    "dts-gen": "^0.5.7",
    "eslint": "^5.0.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^8.0.0-beta.2",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^5.0.0-alpha.2",
    "husky": "^0.15.0-rc.13",
    "jest": "^23.2.0",
    "lodash": "^4.17.5",
    "markdownlint-cli": "^0.10.0",
    "source-map-support": "^0.5.6",
    "ts-jest": "^22.4.6",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1"
  }
}
