{
  "name": "repodog",
  "type": "module",
  "version": "1.0.14-alpha.0",
  "private": true,
  "author": "Dylan Aubrey",
  "engines": {
    "node": "^18.13.0",
    "pnpm": "^7.27.0"
  },
  "scripts": {
    "build": "pnpm run -r build",
    "clean:deps": "pnpm run -r clean:deps && del-cli ./node_modules",
    "clean:dist": "pnpm run -r clean:dist",
    "commit": "commit",
    "cut:changelog": "changelog",
    "cut:post-version": "pnpm run build",
    "lint": "eslint . --ext .ts,.cjs",
    "prepare": "husky install",
    "repodog": "node ./cli/core/bin/repodog.mjs",
    "test": "node --require=suppress-experimental-warnings --experimental-vm-modules node_modules/jest/bin/jest.js",
    "type-check": "tsc --noEmit",
    "validate": "pnpm run build && pnpm run lint && pnpm run type-check && pnpm run test"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.20.13",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@commitlint/prompt-cli": "^17.3.0",
    "@jest/globals": "^29.3.1",
    "@repodog/babel-config": "workspace:./configs/babel-config",
    "@repodog/commitlint-config": "workspace:./configs/commitlint-config",
    "@repodog/eslint-config": "workspace:./configs/eslint-config",
    "@repodog/jest-config": "workspace:./configs/jest-config",
    "@repodog/markdownlint-config": "workspace:./configs/markdownlint-config",
    "@repodog/prettier-config": "workspace:./configs/prettier-config",
    "@repodog/rollup-config": "workspace:./configs/rollup-config",
    "@repodog/ts-config": "workspace:./configs/ts-config",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-image": "^3.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.2.0",
    "@types/jest": "^25.1.3",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "babel-jest": "^29.3.1",
    "babel-plugin-codegen": "^4.1.5",
    "babel-plugin-macros": "^3.1.0",
    "core-js": "^3.27.2",
    "del-cli": "^3.0.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-class-members": "^1.16.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.3.0",
    "eslint-plugin-unicorn": "^45.0.2",
    "generate-changelog": "^1.8.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "markdownlint-cli": "^0.32.2",
    "prettier": "^2.8.1",
    "rollup": "^3.7.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "suppress-experimental-warnings": "^1.1.3",
    "typescript": "^5.0.3"
  }
}