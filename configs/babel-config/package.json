    "@babel/preset-env": "<8",
    "@babel/preset-react": "<8",
    "@babel/preset-typescript": "<8",
    "@babel/runtime": "<8",
    "@rollup/plugin-babel": "<7",
    "babel-plugin-codegen": "<5",
    "babel-plugin-macros": "<4"
